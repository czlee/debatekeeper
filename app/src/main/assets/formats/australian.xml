<?xml version='1.0' encoding='UTF-8'?>
<debate-format schema-version="2.1">
  <name>Australian Easters</name>
  <info>
    <region>Australia</region>
    <level>University</level>
    <used-at>Australian Intervarsity Debating Championships (Easters)</used-at>
    <description>3 vs 3, 8-minute speeches, no replies, no POIs</description>
  </info>
  <prep-time length="30:00"/>
  <speech-types>
    <speech-type ref="all" length="8:00" first-period="normal">
      <bell time="6:00" number="1" next-period="warning"/>
      <bell time="finish" number="2" next-period="overtime"/>
    </speech-type>
  </speech-types>
  <speeches>
    <speech type="all">
      <name>1st Affirmative</name>
    </speech>
    <speech type="all">
      <name>1st Negative</name>
    </speech>
    <speech type="all">
      <name>2nd Affirmative</name>
    </speech>
    <speech type="all">
      <name>2nd Negative</name>
    </speech>
    <speech type="all">
      <name>3rd Affirmative</name>
    </speech>
    <speech type="all">
      <name>3rd Negative</name>
    </speech>
  </speeches>
</debate-format>
